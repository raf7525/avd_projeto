# Arquivo de configuração de ambiente (.env)
# Copie para .env e ajuste conforme necessário

# === DATABASE CONFIGURATION ===
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=mlflow
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# === MLFLOW CONFIGURATION ===
MLFLOW_BACKEND_STORE_URI=postgresql://user:password@localhost:5432/mlflow
MLFLOW_DEFAULT_ARTIFACT_ROOT=s3://mlflow-artifacts
MLFLOW_TRACKING_URI=http://localhost:5000

# === MINIO/S3 CONFIGURATION ===
AWS_ACCESS_KEY_ID=minioadmin
AWS_SECRET_ACCESS_KEY=minioadmin
MLFLOW_S3_ENDPOINT_URL=http://localhost:9000

# === FASTAPI CONFIGURATION ===
FASTAPI_HOST=0.0.0.0
FASTAPI_PORT=8060
FASTAPI_RELOAD=True

# === JUPYTER CONFIGURATION ===
JUPYTER_PORT=1010
JUPYTER_ENABLE_LAB=yes

# === THINGSBOARD CONFIGURATION ===
TB_HOST=localhost
TB_PORT=8080
TB_USERNAME=tenant@thingsboard.org
TB_PASSWORD=tenant

# === TRENDZ ANALYTICS CONFIGURATION ===
TRENDZ_HOST=localhost
TRENDZ_PORT=8888
TRENDZ_BASE_URL=http://localhost:8888
TB_API_URL=http://localhost:8080

# === SNOWFLAKE CONFIGURATION (ajustar quando necessário) ===
# SNOWFLAKE_ACCOUNT=your_account.region
# SNOWFLAKE_USER=your_username
# SNOWFLAKE_PASSWORD=your_password
# SNOWFLAKE_DATABASE=your_database
# SNOWFLAKE_SCHEMA=your_schema
# SNOWFLAKE_WAREHOUSE=your_warehouse

# === TRENDZ ANALYTICS CONFIGURATION ===
# TRENDZ_URL=your_trendz_instance_url
# TRENDZ_TOKEN=your_api_token

# === PROJECT SETTINGS ===
PYTHONPATH=/app
PROJECT_NAME=AVD_thermal_Analysis